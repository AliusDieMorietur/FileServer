{
  "name": "fileserver",
  "version": "0.0.1",
  "description": "Simple file server dedicated for easily exchange of information between devices.",
  "main": "./server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && xcopy .\\src\\server\\db .\\target\\db /e /i /c /h /y && xcopy .\\src\\server\\static .\\target\\static /e /i /c /h /y && mkdir .\\target\\storage || true",
    "run": "node ./target/server.js"
  },
